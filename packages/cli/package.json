{
  "name": "foma-setup",
  "version": "0.4.0",
  "description": "CLI to onboard an AI agent into the FoMA ecosystem via OpenClaw",
  "type": "module",
  "bin": {
    "foma-setup": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "bun run index.tsx",
    "build:linux-x64": "bun build --compile --target=bun-linux-x64 index.tsx --outfile foma-setup-linux-x64",
    "build:linux-arm64": "bun build --compile --target=bun-linux-arm64 index.tsx --outfile foma-setup-linux-arm64",
    "build:mac": "bun build --compile --target=bun-darwin-arm64 index.tsx --outfile foma-setup-mac"
  },
  "dependencies": {
    "ethers": "^6.0.0",
    "chalk": "^5.0.0",
    "ink": "^5.1.0",
    "react-devtools-core": "^6.1.0",
    "react": "^18.3.1",
    "@inkjs/ui": "^2.0.0",
    "viem": "^2.45.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/react": "^18.3.0",
    "@types/node": "^20.0.0"
  }
}
